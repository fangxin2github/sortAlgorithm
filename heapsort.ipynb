   "source": [
    "# 利用heapq快速实现堆排序"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "21aefd7a",
   "metadata": {},
   "outputs": [],
   "source": [
    "import heapq"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "c399ab51",
   "metadata": {},
   "outputs": [],
   "source": [
    "def heapsort(nums):\n",
    "    heapq.heapify(nums)\n",
    "    \n",
    "    return [heapq.heappop(nums) for _ in range(len(nums))]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "c026cf8c",
   "metadata": {},
   "outputs": [
    {
     "data": {
